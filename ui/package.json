{
  "name": "everest-ui",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf public/ && rm -rf .cache && gatsby build --prefix-paths",
    "build:staging": "rm -rf public/ && rm -rf .cache && GATSBY_ACTIVE_ENV=staging gatsby build --prefix-paths && node post-build.js",
    "develop": "gatsby develop -p 4000",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "3box": "1.17.1",
    "@apollo/react-hooks": "^3.1.3",
    "@emotion/core": "^10.0.27",
    "@graphprotocol/mutations": "0.0.0-alpha.0",
    "@graphprotocol/mutations-apollo-react": "0.0.0-alpha.0",
    "@reach/dialog": "^0.7.0",
    "@reach/menu-button": "^0.6.4",
    "@reach/router": "^1.2.1",
    "@theme-ui/components": "0.3.1",
    "@walletconnect/web3-provider": "1.0.0-beta.47",
    "@web3-react/core": "^6.0.2",
    "@web3-react/injected-connector": "^6.0.3",
    "@web3-react/network-connector": "^6.0.4",
    "@web3-react/walletconnect-connector": "^6.0.2",
    "@web3-react/walletlink-connector": "^6.0.2",
    "apollo-boost": "^0.4.4",
    "base-x": "^3.0.7",
    "dotenv": "^8.2.0",
    "ethers": "^4.0.40",
    "gatsby": "^2.18.4",
    "gatsby-plugin-emotion": "^4.1.22",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-ipfs": "2.0.2",
    "gatsby-plugin-layout": "1.1.22",
    "gatsby-plugin-manifest": "2.3.3",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-theme-ui": "0.3.0",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-source-graphql": "2.1.28",
    "graphql": "^14.5.8",
    "moment": "2.24.0",
    "numeral": "^2.0.6",
    "qrcode.react": "^1.0.0",
    "query-string": "6.11.0",
    "react": "^16.12.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-textarea-autosize": "^7.1.2",
    "react-toastify": "5.5.0",
    "theme-ui": "^0.3.1",
    "wallet-connect-provider": "0.1.2",
    "walletlink": "^2.0.1",
    "web3-react": "^5.0.5"
  },
  "devDependencies": {
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/everest/ui"
  },
  "bugs": {
    "url": "https://github.com/graphprotocol/everest/ui"
  }
}
